config AOS_BOARD_STARTERKIT
    bool "StarterKit"
    select AOS_MCU_STM32L4XX_CUBE
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_SENSOR
    help

if AOS_BOARD_STARTERKIT
# Configurations for board starterkit

# "BSP SUPPORT FEATURE"
