						<description>16 transfers</description>		
						<value>0x3</value>		
					</enumeratedValue>			
					<enumeratedValue>			
						<name>32_TRANSFERS</name>		
						<description>32 transfers</description>		
						<value>0x4</value>		
					</enumeratedValue>			
					<enumeratedValue>			
						<name>64_TRANSFERS</name>		
						<description>64 transfers</description>		
						<value>0x5</value>		
