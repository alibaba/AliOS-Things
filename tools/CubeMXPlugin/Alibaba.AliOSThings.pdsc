      <accept Cclass="AliOSThings" Cgroup="OPTION" Csub="no_tls"/>
      <accept Cclass="AliOSThings" Cgroup="security" Csub="mbedtls"/>
    </condition>
    <condition id="depend_protocol_linkkit_iotkit">
      <description>depend of protocol iotkit</description>
      <require Cclass="AliOSThings" Cgroup="security" Csub="mbedtls"/>
