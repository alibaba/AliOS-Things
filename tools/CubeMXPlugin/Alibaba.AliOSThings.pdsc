      <require Tcompiler="IAR"/>
    </condition> 
    <condition id="CM4_KEIL">
      <description>Cortex-M4 under Keil MDK</description>      
      <require  Dcore="Cortex-M4"/>
      <require Tcompiler="ARMCC"/>
    </condition>
    <condition id="CM4_IAR">
      <description>Cortex-M4 under IAR</description>      
      <require  Dcore="Cortex-M4"/>
      <require Tcompiler="IAR"/>
    </condition>
    <condition id="MCU_FPU">
      <accept Dfpu="FPU"/>
      <accept Dfpu="SP_FPU"/>
      <accept Dfpu="DP_FPU"/>
    </condition>
    <condition id="CM4_FPU_IAR">
      <description>Cortex-M4 MCU with FPU under IAR</description>      
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="IAR"/>
      <require condition="MCU_FPU"/>
    </condition>
    <condition id="CM4_FPU_KEIL">
      <description>Cortex-M4 MCU with FPU under KEIL</description>      
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="ARMCC"/>
      <require condition="MCU_FPU"/>
    </condition>
    <condition id="CM4_NFPU_IAR">
      <description>Cortex-M4 MCU with FPU under IAR</description>      
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="IAR"/>
      <require Dfpu="NO_FPU"/>
    </condition>
    <condition id="CM4_NFPU_KEIL">
      <description>Cortex-M4 MCU with FPU under KEIL</description>      
      <require Dcore="Cortex-M4"/>
      <require Tcompiler="ARMCC"/>
      <require Dfpu="NO_FPU"/>
    </condition>

  </conditions>
