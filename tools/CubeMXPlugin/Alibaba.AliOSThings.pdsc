      <require Cclass="AliOSThings" Cgroup="OPTION" Csub="ALICRYPTO_TEST"/>
    </condition>
    <condition id="ops_hdlc">
      <description>ON if hdlc selected</description>
      <require Cclass="AliOSThings" Cgroup="OPTION" Csub="hdlc"/>
    </condition>
    <condition id="ops__tls">
      <description>ON if no_tls not selected</description>
      <deny Cclass="AliOSThings" Cgroup="OPTION" Csub="no_tls"/>
    </condition>
