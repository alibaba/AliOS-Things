      <require Cclass="AliOSThings" Cgroup="kernel" Csub="rhino"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="hal"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="vfs"/>
      <require Cclass="AliOSThings" Cgroup="utility" Csub="digest_algorithm"/>
    </condition>    
    <condition id="depend_platform_mcu_stm32l4xxcube">
      <description>depend of platform mcu_stm32l4xx_cube</description>
      <require Cclass="AliOSThings" Cgroup="platform" Csub="arch_arm_armv7m"/>
      <require Cclass="AliOSThings" Cgroup="utility" Csub="libc"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="rhino"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="hal"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="vfs"/>
      <require Cclass="AliOSThings" Cgroup="utility" Csub="digest_algorithm"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="modules_fs_kv"/>
    </condition>
    <condition id="depend_kernel_rhino">
      <description>depend of kernel rhino</description>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="vcall"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="kernel_init"/>
    </condition>    
    <condition id="depend_kernel_mdoules_fs_kv">
      <description>depend of kernel mdoules_fs_kv</description>
      <require Cclass="AliOSThings" Cgroup="utility" Csub="log"/>
    </condition>
    <condition id="depend_kernel_vfs_Keil">
      <description>depend of kernel vfs</description>
      <!--Switch On below requirement if use PDSC directly in PACK Installer
      <require condition="ARM_Compiler"/>
      -->
      <deny Cclass="AliOSThings" Cgroup="kernel" Csub="vfs_device"/>
    </condition>
    <condition id="depend_kernel_vfs_Other">
      <!--Switch On below requirement if use PDSC directly in PACK Installer
      <deny condition="ARM_Compiler"/>
      -->      
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="vfs_device"/> 
    </condition>
    <condition id="depend_kernel_yloop">
      <description>depend of kernel yloop</description>
      <require Cclass="AliOSThings" Cgroup="utility" Csub="log"/>
      <require Cclass="AliOSThings" Cgroup="kernel" Csub="vfs"/>
    </condition>
    <!-- Select TLS or not:
         if user select option no_tls then there is not necessary select mbedtls
    -->
