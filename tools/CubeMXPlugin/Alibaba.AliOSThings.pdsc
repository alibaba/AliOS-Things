    <condition id="depend_protocol_linkkit_dm">
      <require Cclass="AliOSThings" Cgroup="framework" Csub="common"/>
    </condition>    
    <condition id="cond_fota_socket__socket_stand">
    <description>condition one of fota_socket need socket stand in most case</description>
      <deny Cclass="AliOSThings" Cgroup="OPTION" Csub="STM32_NONSTD_SOCKET"/>
      <require Cclass="AliOSThings" Cgroup="framework" Csub="fota_download_http_socket_opt" Cvariant="stand"/>     
    </condition>
    <condition id="cond_fota_socket__socket_stm32wifi">
      <description>condition one of fota_socket need socket stm32wifi if option STM32_NONSTD_SOCKET selected</description>
      <require Cclass="AliOSThings" Cgroup="OPTION" Csub="STM32_NONSTD_SOCKET"/>
      <require Cclass="AliOSThings" Cgroup="framework" Csub="fota_download_http_socket_opt" Cvariant="stm32wifi"/>      
    </condition>
    <condition id="depend_framework_fota_download_http_socket">
      <description>fota socket need one of below condition</description>
      <accept condition="cond_fota_socket__socket_stand"/>    
      <accept condition="cond_fota_socket__socket_stm32wifi"/>
    </condition> 
    <condition id="cond_fota_platform__mqtt">
