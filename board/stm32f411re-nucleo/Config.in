config AOS_BOARD_STM32F411RE_NUCLEO
    bool "stm32f411re-nucleo"
    select AOS_MCU_STM32F4XX_CUBE
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_SAL if AOS_NETWORK_SAL
    select AOS_COMP_NETMGR if AOS_NETWORK_SAL
    help

if AOS_BOARD_STM32F411RE_NUCLEO
# Configurations for board stm32f411re-nucleo

# "BSP SUPPORT FEATURE"
config BSP_SUPPORT_UART
    bool
    default y

config BSP_SUPPORT_FLASH
    bool
    default y

config BSP_SUPPORT_GPIO
    bool
    default y

config BSP_SUPPORT_I2C
    bool
    default y

config BSP_SUPPORT_WIFI
    bool
    default y

config BSP_WIFI_MODULE
    string
    depends on BSP_SUPPORT_WIFI
    default "wifi.mk3060"

endif
