config AOS_BOARD_STM32F429ZI_NUCLEO
    bool "stm32f429zi-nucleo"
    select AOS_MCU_STM32F4XX_CUBE
    select AOS_COMP_KERNEL_INIT
    select AOS_COMP_SAL if AOS_NETWORK_SAL
    select AOS_COMP_LWIP if AOS_NETWORK_SAL
    select AOS_COMP_NETMGR
    help

if AOS_BOARD_STM32F429ZI_NUCLEO
# Configurations for board stm32f429zi-nucleo

# Set default value of AOS_NETWORK_SAL
config AOS_NETWORK_SAL
    bool "AOS_NETWORK_SAL"
    default n

config SAL_MODULE
    string
    default "wifi.mk3060"

config DEBUG_CONFIG_PANIC
    int "DEBUG_CONFIG_PANIC"
    default 1

config DEBUG_CONFIG_BACKTRACE
    int "DEBUG_CONFIG_BACKTRACE"
    default 1

menu "BSP SUPPORT FEATURE"

config BSP_SUPPORT_UART
    bool "BSP_SUPPORT_UART"
    default y

config BSP_SUPPORT_FLASH
    bool "BSP_SUPPORT_FLASH"
    default y

config BSP_SUPPORT_GPIO
    bool "BSP_SUPPORT_GPIO"
    default y

config BSP_SUPPORT_I2C
    bool "BSP_SUPPORT_I2C"
    default y

config BSP_SUPPORT_WIFI
    bool "BSP_SUPPORT_WIFI"
    default y

endmenu

endif
